{
  "name": "janet",
  "version": "3.0.0",
  "description": "Janet is a bot for Discord, written in JavaScript using the Discord.js, Typescript, Shoukak (Lavalink) library.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run start:bot",
    "start:disco": "npm run start:bot --disco=true",
    "start:bot": "npm run build && node --enable-source-maps dist/index.js",
    "build": "tsc --project tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathan-edwards/janet.git"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "lavalink",
    "shoukaku",
    "lavamusic",
    "typescript",
    "prisma"
  ],
  "author": "nathan-edwards",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/nathan-edwards/janet/issues"
  },
  "homepage": "https://github.com/nathan-edwards/janet#readme",
  "devDependencies": {
    "@types/node": "^18.14.5",
    "@types/request": "^2.48.8",
    "@types/signale": "^1.4.4",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "axios": "^1.3.4",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "moment": "^2.29.4",
    "pm2": "^5.3.0",
    "request": "^2.88.2",
    "shoukaku": "^3.3.1",
    "signale": "^1.4.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "signale": {
    "displayScope": true,
    "displayBadge": true,
    "displayDate": true,
    "displayFilename": true,
    "displayLabel": true,
    "displayTimestamp": true,
    "underlineLabel": true
  }
}
